<?xml version="1.0" encoding="UTF-8"?>
<catalog name="GBR1 2.0 catalog" version="1.0.1"
			xmlns="http://www.unidata.ucar.edu/namespaces/thredds/InvCatalog/v1.0"
					xmlns:xlink="http://www.w3.org/1999/xlink">

	<service name="all" base="" serviceType="compound">
		<service name="odap" serviceType="OpenDAP" base="/thredds/dodsC/" />
		<service name="dap4" serviceType="DAP4" base="/thredds/dap4/" />
		<service name="http" serviceType="HTTPServer" base="/thredds/fileServer/" />
		<service name="wcs" serviceType="WCS" base="/thredds/wcs/" />
		<service name="wms" serviceType="WMS" base="/thredds/wms/" />
		<service name="ncss" serviceType="NetcdfSubset" base="/thredds/ncss/" />
		<service name="latest" serviceType="Resolver" base="" />
	</service>

	<service name="dap" base="" serviceType="compound">
		<service name="odap" serviceType="OpenDAP" base="/thredds/dodsC/" />
		<service name="dap4" serviceType="DAP4" base="/thredds/dap4/" />
	</service>

	<dataset name="eReefs GBR1 Hydrodynamic v2.0"
		serviceName="all"
		ID="gbr1_2.0_DatasetScan_agg"
		urlPath="gbr1_2.0" >
		<netcdf xmlns="http://www.unidata.ucar.edu/namespaces/netcdf/ncml-2.2">
			<aggregation dimName="time" type="joinExisting">
				<scan location="/usr/local/tomcat/content/thredds/public/fx3/gbr1_2.0" suffix=".nc" subdirs="false"/>
			</aggregation>
		</netcdf>
	</dataset>
	<datasetScan name="eReefs GBR1 Hydrodynamic v2.0 model data" 
			ID="gbr1_2.0_DatasetScan"
			path="gbr1_2.0_All"
			location="content/fx3/gbr1_2.0">
		<metadata inherited="true">
			<serviceName>all</serviceName>
			<authority>au.csiro</authority>
			<dataType>GRID</dataType>
			<documentation xlink:href="/thredds/fileServer/licenses/fx3_readme.txt" xlink:title="README"/>
			<documentation type="rights" xlink:href="/thredds/fileServer/licenses/fx3_licence.txt" xlink:title="Licence"/>
			<documentation type="history">
				http://www.emg.cmar.csiro.au/www/en/emg/software/EMS/hydrodynamics.html
			</documentation>
			<documentation type="summary">
				Files from the GBR1 dataset from the eReefs project. See README linked above for further details.
			</documentation>
			<creator>
				<name>
					Coastal Environmental Modelling, CSIRO Oceans and Atmosphere
				</name>
				<contact url="http://www.emg.cmar.csiro.au" email="cmar-hobart@csiro.au"/>
			</creator>
			<publisher>
				<name>Sven's testing Thredds</name>
				<contact url="http://nci.org.au" email="sven.dowideit@csiro.au"/>
			</publisher>
		</metadata>

		<filter>
			<include wildcard="*.nc"/>
		</filter>
		<addProxies>
			<simpleLatest/>
		</addProxies>

		<namer>
			<regExpOnName regExp="gbr1_simple_(([0-9]{4})-([0-9]{2})-([0-9]{2}))\.nc" replaceString="eReefs GBR1 SHOC Model v2.0 Results for $1" />
		</namer>
		<addTimeCoverage
			datasetNameMatchPattern="gbr1_simple_([0-9]{4})-([0-9]{2})-([0-9]{2})\w*.nc$"
			startTimeSubstitutionPattern="$1-$2-$3T00:00:00+10"
			duration="24 hours"/>
	</datasetScan>

</catalog>
