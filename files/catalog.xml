    <service name="wms" serviceType="WMS" base="/thredds/wms/" />
    <service name="ncss" serviceType="NetcdfSubset" base="/thredds/ncss/" />
  </service>

  <service name="dap" base="" serviceType="compound">
    <service name="odap" serviceType="OpenDAP" base="/thredds/dodsC/" />
    <service name="dap4" serviceType="DAP4" base="/thredds/dap4/" />
  </service>

   <datasetScan name="eReefs GBR1 Hydrodynamic v2.0 model data" ID="gbr1_2.0_DatasetScan"
	   path="gbr1_2.0_All" location="content/fx3/gbr1_2.0">
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <dataType>Grid</dataType>
    </metadata>

    <filter>
      <include wildcard="*.nc"/>
    </filter>
  </datasetScan>

  <datasetRoot path="sven" location="content/sven/" />
  <dataset name="sven" ID="svenDataset"
	  urlPath="sven/gbr4_rivers_simple_2017-10.nc">
    <metadata inherited="true">
      <serviceName>all</serviceName>
      <dataType>Grid</dataType>
    </metadata>
  </dataset>

  <catalogRef xlink:title="Test Enhanced Catalog" xlink:href="enhancedCatalog.xml" name=""/>
